{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": "react-app",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error"
  }
}
